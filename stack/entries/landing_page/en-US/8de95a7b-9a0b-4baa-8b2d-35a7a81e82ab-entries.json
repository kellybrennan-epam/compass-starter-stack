cale":"de","time":"2024-10-07T10:33:48.011Z","user":"userUID","version":2},{"environment":"bltb73a6fe374802283","locale":"de","time":"2024-10-07T10:33:48.011Z","user":"userUID","version":2},{"environment":"bltb377ab306d29e646","locale":"en-US","time":"2024-10-01T14:24:39.528Z","user":"userUID","version":1},{"environment":"bltb73a6fe374802283","locale":"en-US","time":"2024-10-01T14:24:39.528Z","user":"userUID","version":1}]},"bltf3d184bf741a3ad0":{"title":"About Us","url":"/about-us","components":[{"text":{"content":{"type":"doc","uid":"eff406b7dda84555b6a55bd5a0344410","_version":3,"attrs":{},"children":[{"uid":"fb905a2a9b76470da8866e328e4a9960","type":"h1","attrs":{"class-name":"xl:mt-0 mt-4 text-4xl font-bold tracking-tight text-gray-900 sm:mt-0 sm:text-6xl dark:text-white","style":{"text-align":"center"},"redactor-attributes":{"class":"xl:mt-0 mt-4 text-4xl font-bold tracking-tight text-gray-900 sm:mt-0 sm:text-6xl dark:text-white","style":"text-align: center;"}},"children":[{"text":"What We Offer"}]}]},"_metadata":{"uid":"csb7992e79ea528764"},"styles":{"background_color":null}}},{"text_and_image":{"heading":"Remote first","_metadata":{"uid":"cs7b7623bcb5d1d9f1"},"content":"Contentstack has offices in the U.S., Europe, South America, and India, but we also believe in the benefits of working remotely – both for the company and our employees. No matter where you live, you can be part of a global team of top talent, collaborating via the latest and best applications during the week and meeting in person at events throughout the year.","icon":null,"image":{"uid":"blt5996fe0e80825027","created_at":"2024-03-14T06:57:24.920Z","updated_at":"2024-03-14T06:57:24.920Z","content_type":"image/webp","file_size":"45360","tags":[],"filename":"about-us-1.webp","url":"https://images.contentstack.io/v3/assets/<api_key>/<asset_uid>/<entity_uid>/<asset_name>","ACL":[],"is_dir":false,"parent_uid":"blte9eefa34e3b3c3df","_version":1,"title":"about-us-1.webp","publish_details":[{"environ